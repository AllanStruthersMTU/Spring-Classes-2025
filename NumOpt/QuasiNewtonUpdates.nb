Cell[BoxData[{
 RowBox[{
  RowBox[{"SR1BUpdate", "[", 
   RowBox[{"B_", ",", 
    RowBox[{"{", 
     RowBox[{"s_", ",", "y_"}], "}"}]}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"r", "=", 
      RowBox[{"y", "-", 
       RowBox[{"B", ".", "s"}]}]}], "}"}], ",", 
    RowBox[{"B", "+", " ", 
     FractionBox[
      RowBox[{"KroneckerProduct", "[", 
       RowBox[{"r", ",", " ", "r"}], "]"}], 
      RowBox[{"r", ".", "s"}]]}]}], "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"BFGSBUpdate", "[", 
   RowBox[{"B_", ",", 
    RowBox[{"{", 
     RowBox[{"s_", ",", "y_"}], "}"}]}], "]"}], ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"\[Rho]", "=", 
       RowBox[{"1", "/", 
        RowBox[{"s", ".", "y"}]}]}], ",", 
      RowBox[{"Bs", "=", 
       RowBox[{"B", ".", "s"}]}]}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{"If", "[", 
     RowBox[{
      RowBox[{"\[Rho]", ">", "0"}], ",", " ", 
