       RowBox[{
        RowBox[{
         FractionBox[
          RowBox[{"2", "\[Pi]", " ", "\[ImaginaryI]"}], 
          SuperscriptBox["\[Epsilon]", 
           RowBox[{"n", "-", "1"}]]], 
         RowBox[{
          SubsuperscriptBox["\[Integral]", "0", "1"], 
          RowBox[{
           SuperscriptBox["E", 
            RowBox[{"2", 
             RowBox[{"(", 
              RowBox[{"n", "-", "1"}], ")"}], "\[Pi]", " ", "\[ImaginaryI]", 
             " ", "t"}]], "dt"}]}]}], "=", 
        RowBox[{
         FractionBox[
          RowBox[{"2", "\[Pi]", " ", "\[ImaginaryI]"}], "\[Epsilon]"], 
         FractionBox[
