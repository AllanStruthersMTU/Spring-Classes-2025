Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   SubsuperscriptBox[
    RowBox[{
     RowBox[{
      RowBox[{
       SubsuperscriptBox["\[Integral]", "0", "1"], 
       RowBox[{
        RowBox[{"f", "[", 
         RowBox[{"z", "[", "t", "]"}], "]"}], "*", 
        RowBox[{
         RowBox[{"z", "'"}], "[", "t", "]"}], "dt"}]}], "=", 
      RowBox[{
       RowBox[{
        SubsuperscriptBox["\[Integral]", "0", "1"], 
        RowBox[{
         FractionBox["r", 
          SuperscriptBox[
           RowBox[{"(", 
            RowBox[{"\[Epsilon]", " ", 
             SuperscriptBox["E", 
              RowBox[{"2", "\[Pi]", " ", "\[ImaginaryI]", " ", "t"}]]}], 
            ")"}], "n"]], "*", "2", "\[Pi]", " ", "\[ImaginaryI]", " ", 
         "\[Epsilon]", " ", 
