Cell[TextData[{
 "A counter clockwise parametrization for a tiny circle around a pole ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"f", "[", "z", "]"}], "=", 
    FractionBox["r", 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"z", "-", "c"}], ")"}], "n"]]}], TraditionalForm]],
  ExpressionUUID->"6562233f-080a-4610-abb1-015e23b09165"],
 " is ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"z", "(", "t", ")"}], "=", 
    RowBox[{"c", "+", 
     RowBox[{"\[Epsilon]", " ", 
      SuperscriptBox["E", 
       RowBox[{"2", "\[Pi]", " ", "\[ImaginaryI]", " ", "t"}]]}]}]}], 
   TraditionalForm]],ExpressionUUID->"c4d8b0bf-2ba9-4136-8e14-dd7670dd45f0"],
 " with ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"z", "'"}], "[", "t", "]"}], "=", 
    RowBox[{"2", "\[Pi]", " ", "\[ImaginaryI]", " ", "\[Epsilon]", " ", 
     SuperscriptBox["E", 
      RowBox[{"2", "\[Pi]", " ", "\[ImaginaryI]", " ", "t"}]]}]}], 
   TraditionalForm]],ExpressionUUID->"9e6ca602-f17d-4e13-8d2b-d03f08fc77a9"],
 "."
}], "Text",
