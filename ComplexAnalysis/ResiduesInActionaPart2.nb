Cell[TextData[{
 "Putting this together we get\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["\[ContourIntegral]", "\[CapitalGamma]"], 
     RowBox[{
      RowBox[{"log", "(", "z", ")"}], 
      RowBox[{"f", "(", "z", ")"}], "dz"}]}], "=", 
    RowBox[{
     RowBox[{"2", "\[Pi]", " ", "\[ImaginaryI]", " ", 
      RowBox[{
       SubsuperscriptBox["\[Integral]", "0", "R"], 
       RowBox[{
        RowBox[{"f", "(", "z", ")"}], "dz"}]}]}], "+", 
     RowBox[{
      SubscriptBox["\[Integral]", 
       SubscriptBox["\[CapitalGamma]", "1"]], 
      RowBox[{
       RowBox[{"log", "(", "z", ")"}], 
       RowBox[{"f", "(", "z", ")"}], "dz"}]}]}]}], TraditionalForm]],
  ExpressionUUID->"e7bb1e85-3ffd-4846-8df8-6a7331597964"],
 "\nIf ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"f", "(", "z", ")"}], " ", "\[Rule]", "0"}], TraditionalForm]],
  ExpressionUUID->"b018061e-ebab-4810-8f08-cc15238d6d2f"],
 " fast enough as ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"|", "z", "|"}], "\[Rule]", "\[Infinity]"}], TraditionalForm]],
  ExpressionUUID->"a0cb098b-4385-4640-8149-6029ec08e5a2"],
 " then ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["\[Integral]", 
      SubscriptBox["\[CapitalGamma]", "1"]], 
     RowBox[{
      RowBox[{"log", "(", "z", ")"}], 
      RowBox[{"f", "(", "z", ")"}], "dz"}]}], "->", "0"}], TraditionalForm]],
  ExpressionUUID->"854105dd-47dc-481e-9619-c6c2855899c7"],
 " and we can evaluate\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubsuperscriptBox["\[Integral]", "0", "\[Infinity]"], 
     RowBox[{
      RowBox[{"f", "(", "z", ")"}], "dz"}]}], "=", 
    RowBox[{
     FractionBox["1", 
      RowBox[{"2", "\[Pi]", " ", "\[ImaginaryI]"}]], 
     RowBox[{
      SubscriptBox["\[ContourIntegral]", "\[CapitalGamma]"], 
      RowBox[{
       RowBox[{"log", "(", "z", ")"}], 
       RowBox[{"f", "(", "z", ")"}], "dz"}]}]}]}], TraditionalForm]],
  ExpressionUUID->"50ced264-a7af-4ba0-90e8-98c8e4a52bee"],
 "\nusing the residue theorem!"
}], "Text",
