Cell[TextData[{
 "The straight line portions of ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[ContourIntegral]", "\[CapitalGamma]"], TraditionalForm]],
  ExpressionUUID->"ad034eb1-9273-4fd3-bfec-0d14be91834a"],
 " cancel unless there is a branch cut along the real axis! if ",
 Cell[BoxData[
  FormBox["f", TraditionalForm]],ExpressionUUID->
  "0ba48400-6e48-4717-8eca-e6e0f5239a25"],
 " has no branch cuts then we can be weird and put the branch cut for log \
along the real axis and\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["\[Integral]", "\[CapitalGamma]"], 
     RowBox[{
      RowBox[{"log", "(", "z", ")"}], 
      RowBox[{"f", "(", "z", ")"}], "dz"}]}], "=", 
    RowBox[{
     RowBox[{
      SubsuperscriptBox["\[Integral]", "\[Epsilon]", "R"], 
      RowBox[{
       RowBox[{"log", "(", 
        RowBox[{"x", "+", 
         RowBox[{"\[Epsilon]", " ", "\[ImaginaryI]"}]}], ")"}], 
       RowBox[{"f", "(", 
        RowBox[{"x", "+", 
         RowBox[{"\[Epsilon]", " ", "\[ImaginaryI]"}]}], ")"}], "dx"}]}], "+", 
     RowBox[{
      SubsuperscriptBox["\[Integral]", "R", "\[Epsilon]"], 
      RowBox[{
       RowBox[{"log", "(", 
        RowBox[{"x", "-", 
         RowBox[{"\[Epsilon]", " ", "\[ImaginaryI]"}]}], ")"}], 
       RowBox[{"f", "(", 
        RowBox[{"x", "-", 
         RowBox[{"\[Epsilon]", " ", "\[ImaginaryI]"}]}], ")"}], "dx"}]}], "+", 
     RowBox[{
      SubscriptBox["\[Integral]", 
       SubscriptBox["\[CapitalGamma]", "1"]], 
      RowBox[{
       RowBox[{"log", "(", "z", ")"}], 
       RowBox[{"f", "(", "z", ")"}], " ", "dz"}]}], "+", 
     RowBox[{
      SubscriptBox["\[Integral]", 
       SubscriptBox["\[CapitalGamma]", "3"]], 
      RowBox[{
       RowBox[{"log", "(", "z", ")"}], 
       RowBox[{"f", "(", "z", ")"}], "dz"}]}]}]}], TraditionalForm]],
  ExpressionUUID->"742d741f-6091-4459-8068-d643d814d7d7"],
 ".\n\t\nFlipping the order of the limits in the second term gives\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["\[Integral]", "\[CapitalGamma]"], 
     RowBox[{
      RowBox[{"log", "(", "z", ")"}], 
      RowBox[{"f", "(", "z", ")"}], "dz"}]}], "=", 
    RowBox[{
     StyleBox[
      RowBox[{
       SubsuperscriptBox["\[Integral]", "\[Epsilon]", "R"], 
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{
           RowBox[{"log", "(", 
            RowBox[{"x", "+", 
             RowBox[{"\[Epsilon]", " ", "\[ImaginaryI]"}]}], ")"}], 
           RowBox[{"f", "(", 
            RowBox[{"x", "+", 
             RowBox[{"\[Epsilon]", " ", "\[ImaginaryI]"}]}], ")"}]}], "-", 
          RowBox[{
           RowBox[{"log", "(", 
            RowBox[{"x", "-", 
             RowBox[{"\[Epsilon]", " ", "\[ImaginaryI]"}]}], ")"}], 
           RowBox[{"f", "(", 
            RowBox[{"x", "-", 
             RowBox[{"\[Epsilon]", " ", "\[ImaginaryI]"}]}], ")"}]}]}], ")"}],
         "dx"}]}],
      FontColor->RGBColor[1, 0, 0]], "+", 
     RowBox[{
      SubscriptBox["\[Integral]", 
       SubscriptBox["\[CapitalGamma]", "1"]], 
      RowBox[{
       RowBox[{"log", "(", "z", ")"}], 
       RowBox[{"f", "(", "z", ")"}], " ", "dz"}]}], "+", 
     StyleBox[
      RowBox[{
       SubscriptBox["\[Integral]", 
        SubscriptBox["\[CapitalGamma]", "3"]], 
       RowBox[{
        RowBox[{"log", "(", "z", ")"}], 
        RowBox[{"f", "(", "z", ")"}], "dz"}]}],
      FontColor->RGBColor[0, 1, 0]]}]}], TraditionalForm]],ExpressionUUID->
  "59a62a02-38c9-4816-8f1a-7896a8779577"],
 "\n\nSince ",
 Cell[BoxData[
  FormBox["f", TraditionalForm]],ExpressionUUID->
  "d015ffc1-9a85-47e4-9a7e-1e52141567fd"],
 " is continuous let ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Epsilon]", "->", 
    SuperscriptBox["0", "+"]}], TraditionalForm]],ExpressionUUID->
  "71b5bbab-b0b7-41d1-ae8c-a6e10f349f73"],
 " and the red term becomes\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubsuperscriptBox["\[Integral]", "0", "R"], 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        RowBox[{"log", "(", 
         RowBox[{"x", "+", 
          RowBox[{
           SuperscriptBox["0", "+"], " ", "\[ImaginaryI]"}]}], ")"}], "-", 
        RowBox[{"log", "(", 
         RowBox[{"x", "-", 
          RowBox[{
           SuperscriptBox["0", "+"], "\[ImaginaryI]"}]}], ")"}]}], ")"}], 
      RowBox[{"f", "(", "x", ")"}], "dx"}]}], "=", 
    RowBox[{
     RowBox[{
      SubsuperscriptBox["\[Integral]", "0", "R"], 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"log", "(", "x", ")"}], "-", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{"log", "(", "x", ")"}], "+", 
           RowBox[{"2", "\[Pi]", " ", "\[ImaginaryI]"}]}], ")"}]}], ")"}], 
       RowBox[{"f", "(", "x", ")"}], "dx"}]}], "=", 
     RowBox[{"2", "\[Pi]", " ", "\[ImaginaryI]", " ", 
      RowBox[{
       SubsuperscriptBox["\[Integral]", "0", "R"], 
       RowBox[{
        RowBox[{"f", "(", "x", ")"}], "dx"}]}]}]}]}], TraditionalForm]],
  ExpressionUUID->"efa86fa9-0d86-4540-9495-27b014965812"],
 "\nAs ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Epsilon]", "->", 
    SuperscriptBox["0", "+"]}], TraditionalForm]],ExpressionUUID->
  "a3429456-dda0-4f39-92e1-70703eadd852"],
 " the green term because to zero because ",
 Cell[BoxData[
  FormBox[
   RowBox[{"dz", "=", 
    RowBox[{"\[ImaginaryI]", " ", "\[Epsilon]", " ", "\[Pi]", " ", 
     SuperscriptBox["\[ExponentialE]", 
      RowBox[{"\[ImaginaryI]", " ", "\[Pi]", " ", "t"}]], " "}]}], 
   TraditionalForm]],ExpressionUUID->"b0c82a6c-63fd-416f-9c67-fe277f294793"],
 "\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"|", 
    RowBox[{
     RowBox[{"log", "(", 
      RowBox[{"\[Epsilon]", " ", 
       SuperscriptBox["\[ExponentialE]", 
        RowBox[{"i", " ", "\[Pi]", " ", "t"}]]}], ")"}], 
     RowBox[{"f", "(", 
      RowBox[{"\[Epsilon]", " ", 
       SuperscriptBox["\[ExponentialE]", 
        RowBox[{"\[ImaginaryI]", " ", "\[Pi]", " ", "t"}]]}], ")"}]}], "|", 
    RowBox[{"\[TildeTilde]", " ", 
     RowBox[{"-", 
      RowBox[{"log", "(", "\[Epsilon]", ")"}]}]}], "|", 
    RowBox[{"f", "(", "0", ")"}], "|"}], TraditionalForm]],ExpressionUUID->
  "3a6e4baf-fdb6-4f12-951a-443a05eb1401"],
 "\nso \n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"|", 
     RowBox[{
      SubscriptBox["\[Integral]", 
       SubscriptBox["\[CapitalGamma]", "3"]], 
      RowBox[{
       RowBox[{"log", "(", "z", ")"}], 
       RowBox[{"f", "(", "z", ")"}], "dz"}]}], "|", 
     RowBox[{"\[LessEqual]", 
      RowBox[{
       RowBox[{"-", "\[Epsilon]"}], " ", "\[Pi]", " ", 
       RowBox[{"log", "(", "\[Epsilon]", ")"}]}]}], "|", 
     RowBox[{"f", "(", "0", ")"}], "|"}], "\[Rule]", "0"}], TraditionalForm]],
  FontColor->GrayLevel[0],ExpressionUUID->
  "fe91bd3c-c5cb-477c-9b12-cdafb26619db"]
}], "Text",
 CellChangeTimes->{{3.951380747089572*^9, 3.9513807957164445`*^9}, {
  3.951380853849083*^9, 3.951380973124853*^9}, {3.9513810069309125`*^9, 
  3.9513810497410192`*^9}, {3.951381102895454*^9, 3.9513812505459967`*^9}, {
