57a4e61329b7"],

Cell[TextData[{
 "This is much more complicated than\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Integral]", 
     RowBox[{
      SuperscriptBox["e", 
       RowBox[{"\[Alpha]", " ", "x"}]], 
      RowBox[{"sin", "(", 
       RowBox[{"\[Beta]", " ", "x"}], ")"}], "dx"}]}], "=", 
    RowBox[{
     RowBox[{"Im", "(", 
      RowBox[{"\[Integral]", 
       RowBox[{
        SuperscriptBox["e", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"\[Alpha]", "+", 
            RowBox[{"\[Beta]", " ", "\[ImaginaryI]"}]}], ")"}], " ", "x"}]], 
        "dx"}]}], ")"}], "=", 
     RowBox[{
      RowBox[{"Im", "(", 
       FractionBox[
        SuperscriptBox["e", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"\[Alpha]", "+", 
            RowBox[{"\[Beta]", " ", "\[ImaginaryI]"}]}], ")"}], " ", "x"}]], 
        RowBox[{"\[Alpha]", " ", "+", " ", 
         RowBox[{"\[Beta]", " ", "\[ImaginaryI]"}]}]], ")"}], "=", 
      RowBox[{"Im", "(", 
       FractionBox[
        RowBox[{
         SuperscriptBox["e", 
          RowBox[{"\[Alpha]", " ", "x"}]], "(", 
         RowBox[{
          RowBox[{"cos", "(", 
           RowBox[{"\[Beta]", " ", "x"}], ")"}], "+", " ", 
          RowBox[{"\[ImaginaryI]", " ", 
           RowBox[{"sin", "(", 
            RowBox[{"\[Beta]", " ", "x"}], ")"}]}]}], ")"}], 
        RowBox[{"\[Alpha]", " ", "+", " ", 
         RowBox[{"\[Beta]", " ", "\[ImaginaryI]"}]}]], ")"}]}]}]}], 
   TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "866c072d-c772-477d-bbe6-0511d3b241f1"],
 "\nand a little bit of complex arithmetic\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     FractionBox[
      RowBox[{
       SuperscriptBox["e", 
        RowBox[{"\[Alpha]", " ", "x"}]], "(", 
       RowBox[{
        RowBox[{"cos", "(", 
         RowBox[{"\[Beta]", " ", "x"}], ")"}], "+", " ", 
        RowBox[{"\[ImaginaryI]", " ", 
         RowBox[{"sin", "(", 
          RowBox[{"\[Beta]", " ", "x"}], ")"}]}]}], ")"}], 
      RowBox[{"\[Alpha]", " ", "+", " ", 
       RowBox[{"\[Beta]", " ", "\[ImaginaryI]"}]}]], 
     FractionBox[
      RowBox[{"\[Alpha]", " ", "-", " ", 
       RowBox[{"\[Beta]", " ", "\[ImaginaryI]"}]}], 
      RowBox[{"\[Alpha]", " ", "-", " ", 
       RowBox[{"\[Beta]", " ", "\[ImaginaryI]"}]}]]}], "=", 
    FractionBox[
     RowBox[{
      RowBox[{
       SuperscriptBox["e", 
        RowBox[{"\[Alpha]", " ", "x"}]], "(", 
       RowBox[{
        RowBox[{"cos", "(", 
         RowBox[{"\[Beta]", " ", "x"}], ")"}], "+", " ", 
        RowBox[{"\[ImaginaryI]", " ", 
         RowBox[{"sin", "(", 
          RowBox[{"\[Beta]", " ", "x"}], ")"}]}]}], ")"}], 
      RowBox[{"(", 
       RowBox[{"\[Alpha]", "-", 
        RowBox[{"\[Beta]", " ", "\[ImaginaryI]"}]}], ")"}]}], 
     RowBox[{
      SuperscriptBox["\[Alpha]", "2"], " ", "+", " ", 
      SuperscriptBox["\[Beta]", "2"]}]]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "78426ffe-9fb1-4e1a-a702-bfff53b38c86"],
 "\nfinished up with a multiplication\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"cos", "(", 
        RowBox[{"\[Beta]", " ", "x"}], ")"}], "+", " ", 
       RowBox[{"\[ImaginaryI]", " ", 
        RowBox[{"sin", "(", 
         RowBox[{"\[Beta]", " ", "x"}], ")"}]}]}], ")"}], 
     RowBox[{"(", 
      RowBox[{"\[Alpha]", "-", 
       RowBox[{"\[Beta]", " ", "\[ImaginaryI]"}]}], ")"}]}], "=", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"\[Alpha]", " ", 
        RowBox[{"cos", "(", 
         RowBox[{"x", " ", "\[Beta]"}], ")"}]}], "+", 
       RowBox[{"\[Beta]", " ", 
        RowBox[{"sin", "(", 
         RowBox[{"x", " ", "\[Beta]"}], ")"}]}]}], ")"}], "+", 
     RowBox[{"\[ImaginaryI]", " ", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"\[Alpha]", " ", 
         RowBox[{"sin", "(", 
          RowBox[{"x", " ", "\[Beta]"}], ")"}]}], "-", 
        RowBox[{"\[Beta]", " ", 
         RowBox[{"cos", "(", 
          RowBox[{"x", " ", "\[Beta]"}], ")"}]}]}], ")"}]}]}]}], 
   TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "ee5b7fb8-dc73-4fbe-b72c-41fc8a37dfea"],
 "\nputting it all together gives\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Integral]", 
     RowBox[{
      SuperscriptBox["e", 
       RowBox[{"\[Alpha]", " ", "x"}]], 
      RowBox[{"sin", "(", 
       RowBox[{"\[Beta]", " ", "x"}], ")"}], "dx"}]}], "=", 
    RowBox[{
     RowBox[{"Im", "(", 
      FractionBox[
       RowBox[{
        RowBox[{
         SuperscriptBox["e", 
          RowBox[{"\[Alpha]", " ", "x"}]], "(", 
         RowBox[{
          RowBox[{"cos", "(", 
           RowBox[{"\[Beta]", " ", "x"}], ")"}], "+", " ", 
          RowBox[{"\[ImaginaryI]", " ", 
           RowBox[{"sin", "(", 
            RowBox[{"\[Beta]", " ", "x"}], ")"}]}]}], ")"}], 
        RowBox[{"(", 
         RowBox[{"\[Alpha]", "-", 
          RowBox[{"\[Beta]", " ", "\[ImaginaryI]"}]}], ")"}]}], 
       RowBox[{
        SuperscriptBox["\[Alpha]", "2"], " ", "+", " ", 
        SuperscriptBox["\[Beta]", "2"]}]], ")"}], "=", 
     FractionBox[
      RowBox[{
       SuperscriptBox["e", 
        RowBox[{"\[Alpha]", " ", "x"}]], "(", 
       RowBox[{
        RowBox[{"\[Alpha]", " ", 
         RowBox[{"sin", "(", 
          RowBox[{"\[Beta]", " ", "x"}], ")"}]}], "-", 
        RowBox[{"\[Beta]", " ", 
         RowBox[{"cos", "(", 
          RowBox[{"\[Beta]", " ", "x"}], ")"}]}]}], ")"}], 
      RowBox[{
       SuperscriptBox["\[Alpha]", "2"], " ", "+", " ", 
       SuperscriptBox["\[Beta]", "2"]}]]}]}], TraditionalForm]],
  ExpressionUUID->"35345b9e-5b0b-49e1-85b5-4a7a1cb1fd1e"]
}], "Text",
