Cell[TextData[{
 "The crazy way to compute\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Integral]", 
    RowBox[{
     SuperscriptBox["e", 
      RowBox[{"\[Alpha]", " ", "x"}]], 
     RowBox[{"sin", "(", 
      RowBox[{"\[Beta]", " ", "x"}], ")"}], "dx"}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "6c25a8f1-be45-4cc6-9bc7-63e3b1d968fb"]
}], "Title",
