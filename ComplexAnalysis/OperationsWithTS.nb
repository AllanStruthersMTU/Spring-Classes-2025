Cell[BoxData[{
 RowBox[{"Clear", "[", 
  RowBox[{"a", ",", "b"}], "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"n", "=", "4"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"TSf", "[", "z_", "]"}], "=", 
   RowBox[{"Sum", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"a", "[", "k", "]"}], 
      SuperscriptBox["z", "k"]}], ",", 
     RowBox[{"{", 
      RowBox[{"k", ",", "1", ",", "n"}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"TSg", "[", "w_", "]"}], "=", 
   RowBox[{"Sum", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"b", "[", "k", "]"}], " ", 
      SuperscriptBox["w", "k"]}], ",", 
     RowBox[{"{", 
      RowBox[{"k", ",", "1", ",", "n"}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{"TableForm", "[", 
  RowBox[{
   RowBox[{"CoefficientList", "[", 
    RowBox[{
     RowBox[{"TSf", "[", 
      RowBox[{"TSg", "[", "z", "]"}], "]"}], ",", "z"}], "]"}], 
   "\[LeftDoubleBracket]", 
   RowBox[{"1", ";;", "n"}], "\[RightDoubleBracket]"}], "]"}]}], "Input",
 CellChangeTimes->{{3.9475831467667875`*^9, 3.947583331131699*^9}, {
  3.9475833784109955`*^9, 3.947583408223632*^9}, {3.947583445071312*^9, 
