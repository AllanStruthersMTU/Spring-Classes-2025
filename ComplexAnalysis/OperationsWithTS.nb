Cell[CellGroupData[{
Cell["Operations with Taylor Series", "Title",
 CellChangeTimes->{{3.9398152070839567`*^9, 3.9398152117860727`*^9}, {
   3.9421541444318066`*^9, 3.9421541513390083`*^9}, {3.944393994649187*^9, 
   3.9443940027240753`*^9}, {3.944566660315626*^9, 3.9445666686709223`*^9}, {
   3.9451640246777716`*^9, 3.945164029754927*^9}, 3.945164250898985*^9, {
   3.946974046790117*^9, 3.9469740486428137`*^9}, {3.947582421308018*^9, 
   3.947582432759435*^9}},ExpressionUUID->"8e791b87-b8fe-4d47-a58b-\
235908e2ee0d"],

Cell[TextData[{
 "If I have a TS (and within the radius of convergence R)\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"f", "(", "z", ")"}], "=", 
    RowBox[{
     SubsuperscriptBox["\[Sum]", 
      RowBox[{"k", "=", "0"}], "\[Infinity]"], 
     SuperscriptBox[
      RowBox[{
       SubscriptBox["a", "k"], "(", 
       RowBox[{"z", "-", 
        SubscriptBox["z", "0"]}], ")"}], "k"]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "e5a7316d-6554-49d4-b5c5-dfc8d4d9606c"],
 "\nthen what should the following be\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"??", 
    RowBox[{"=", 
     RowBox[{
      SubsuperscriptBox["\[Sum]", 
       RowBox[{"k", "=", "0"}], "\[Infinity]"], 
      RowBox[{"k", " ", 
       SuperscriptBox[
        RowBox[{
         SubscriptBox["a", "k"], "(", 
         RowBox[{"z", "-", 
          SubscriptBox["z", "0"]}], ")"}], 
        RowBox[{"k", "-", "1"}]]}]}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "a2af4c77-59eb-43ee-bcca-3c934c41dba2"],
 "\nand \n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"??", 
    RowBox[{"=", 
     RowBox[{
      SubsuperscriptBox["\[Sum]", 
       RowBox[{"k", "=", "0"}], "\[Infinity]"], 
      RowBox[{
       FractionBox[
        SubscriptBox["a", "k"], 
        RowBox[{"k", "+", "1"}]], 
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"z", "-", 
          SubscriptBox["z", "0"]}], ")"}], 
        RowBox[{"k", "+", "1"}]]}]}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "002e4c11-6bd2-4bae-853a-7f1220ba506f"]
}], "Text",
 CellChangeTimes->{{3.9475824371273365`*^9, 
  3.9475826988653526`*^9}},ExpressionUUID->"34c977bb-d424-47b4-9353-\
a46fa31ef571"],

Cell[TextData[{
 "Can we write down a a TS for the inverse of ",
 Cell[BoxData[
  FormBox[
   RowBox[{"f", "(", "z", ")"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "51b625de-b227-4fe5-a8c4-0354a5147e9e"],
 ".  To keep it simple lets center both series at ",
 Cell[BoxData[
  FormBox[
   RowBox[{"z", "=", "0"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "447632a0-c0ad-4fe4-b853-aae0bc9d1675"],
 " and assume ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"f", "(", "0", ")"}], "=", "0"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "6512748f-dc0e-421f-a813-01963dfbff35"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"g", "(", "0", ")"}], "=", "0"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "d46f1d0d-f315-4770-a9cf-013cbc5e1048"],
 " and not worry about the convergence. We have\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"f", "(", "z", ")"}], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["a", "0"], "+", 
      RowBox[{
       SubsuperscriptBox["\[Sum]", 
        RowBox[{"k", "=", "1"}], "\[Infinity]"], 
       RowBox[{
        SubscriptBox["a", "k"], " ", 
        SuperscriptBox["z", "k"]}]}]}], "=", 
     RowBox[{
      SubsuperscriptBox["\[Sum]", 
       RowBox[{"k", "=", "1"}], "\[Infinity]"], 
      RowBox[{
       SubscriptBox["a", "k"], " ", 
       SuperscriptBox["z", "k"]}]}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "db17d3f4-0df4-482e-9c6b-fe130d3701c9"],
 "\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"g", "(", "w", ")"}], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["b", "0"], "+", 
      RowBox[{
       SubsuperscriptBox["\[Sum]", 
        RowBox[{"k", "=", "1"}], "\[Infinity]"], 
       RowBox[{
        SubscriptBox["b", "k"], " ", 
        SuperscriptBox["w", "k"]}]}]}], "=", 
     RowBox[{
      SubsuperscriptBox["\[Sum]", 
       RowBox[{"k", "=", "1"}], "\[Infinity]"], 
      RowBox[{
       SubscriptBox["b", "k"], " ", 
       SuperscriptBox["w", "k"]}]}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "b2863920-f18f-4f38-a7b0-e0460328d3be"],
 "\nand ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"f", "(", 
     RowBox[{"g", "(", "z", ")"}], ")"}], "=", "z"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "8ffd05c7-6657-47bb-82f2-fc3a69139f6c"],
 ".  Just plug them in\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"0", "+", 
     RowBox[{
      SubscriptBox["a", "1"], "(", 
      RowBox[{
       SubsuperscriptBox["\[Sum]", 
        RowBox[{"k", "=", "1"}], "\[Infinity]"], 
       RowBox[{
        SubscriptBox["b", "k"], " ", 
        SuperscriptBox["z", "k"]}]}], ")"}], "+", 
     SuperscriptBox[
      RowBox[{
       SubscriptBox["a", "2"], "(", 
       RowBox[{
        SubsuperscriptBox["\[Sum]", 
         RowBox[{"k", "=", "1"}], "\[Infinity]"], 
        RowBox[{
         SubscriptBox["b", "k"], " ", 
         SuperscriptBox["z", "k"]}]}], ")"}], "2"], "+", "\[Ellipsis]"}], "=", 
    RowBox[{"0", "+", " ", 
     RowBox[{"1", " ", "z"}], "+", 
     RowBox[{"0", 
      SuperscriptBox["z", "2"]}], "+", "\[Ellipsis]"}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "6310f0c2-3634-4cbc-94d7-e0ead5b1ccb6"],
 "\nand tidy \n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      SubscriptBox["a", "0"], "+", 
      RowBox[{
       SubscriptBox["a", "1"], 
       SubscriptBox["b", "0"]}]}], ")"}], "+"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "cfa5283d-f8e0-4c24-9c9e-b8723561ee0f"]
}], "Text",
 CellChangeTimes->{{3.9475827100398264`*^9, 3.9475828027544265`*^9}, {
  3.9475828338597193`*^9, 3.9475831406476192`*^9}, {3.947583536270722*^9, 
  3.947583632572672*^9}},ExpressionUUID->"66ba4cd3-fb2d-430c-9863-\
4e4ec2ba54f8"],

