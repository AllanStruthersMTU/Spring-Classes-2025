Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"TS", "(", "z", ")"}], "=", 
    RowBox[{
     SubsuperscriptBox["\[Sum]", 
      RowBox[{"k", "=", 
       RowBox[{"-", "\[Infinity]"}]}], "\[Infinity]"], 
     SuperscriptBox[
      RowBox[{
       SubscriptBox["a", 
        RowBox[{"k", " "}]], "(", 
       RowBox[{"z", "-", 
        SubscriptBox["z", "0"]}], ")"}], "k"]}]}], TraditionalForm]],
  ExpressionUUID->"ea870f46-dee0-421b-b83a-3b95e8093a04"],
 " on disk ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[ScriptCapitalD]", "=", 
    RowBox[{"{", 
     RowBox[{"z", ":", 
      RowBox[{"|", 
       RowBox[{"z", "-", 
        SubscriptBox["z", "0"]}], "|", 
       RowBox[{"<", " ", "R"}]}]}], "}"}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "3e0f52bd-ec15-466c-8d23-8321c564a3aa"],
 "\nwhere ",
 Cell[BoxData[
  FormBox["f", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "e454aadc-6843-4c27-a346-2a6cd4e296c4"],
 " is analytic on \[ScriptCapitalD] and c is any simple CCW contour inside \
\[ScriptCapitalD] \n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["a", "k"], "=", 
    RowBox[{
     FractionBox[
      RowBox[{
       SuperscriptBox["f", 
        RowBox[{"(", "k", ")"}]], "(", 
       SubscriptBox["z", "0"], ")"}], 
      RowBox[{"k", "!"}]], "=", 
     RowBox[{
      FractionBox["1", 
       RowBox[{"2", "\[Pi]", " ", "\[ImaginaryI]"}]], 
      RowBox[{
       SubscriptBox["\[Integral]", "c"], 
       RowBox[{
        FractionBox[
         RowBox[{"f", "(", "\[Zeta]", ")"}], 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{"\[Zeta]", "-", 
            SubscriptBox["z", "0"]}], ")"}], 
          RowBox[{"k", "+", "1"}]]], "d\[Zeta]"}]}]}]}]}], TraditionalForm]],
  ExpressionUUID->"f5959c40-efc0-48d0-84b2-f7f74e22dfb9"],
 "."
}], "Text",
