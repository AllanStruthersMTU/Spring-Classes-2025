 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox[
     RowBox[{"(", 
      RowBox[{"a", ",", "b"}], ")"}], "c"], "*", GridBox[{
      {
       SubscriptBox[
        RowBox[{"(", 
         RowBox[{"c", ",", "d"}], ")"}], "c"], "=", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          RowBox[{"a", "*", "c"}], "-", 
          RowBox[{
           OverscriptBox["d", "_"], "*", "b"}]}], ",", " ", 
         RowBox[{
          RowBox[{"d", "*", "a"}], "+", 
          RowBox[{"b", "*", 
           OverscriptBox["c", "_"]}]}]}], ")"}]}
     }]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "3c1392bd-6975-4af1-80ab-883c0dc2e642"],
 "\n\t\t\t\t\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"(", 
      RowBox[{"a", "+", 
       RowBox[{"b", " ", "\[ImaginaryI]"}]}], ")"}], "*", 
     RowBox[{"(", 
      RowBox[{"c", "+", 
       RowBox[{"d", " ", "\[ImaginaryI]"}]}], ")"}]}], "="}], 
   TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "910fb82f-3c62-4a51-a41c-024364917bc0"],
 " ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"(", 
     RowBox[{
